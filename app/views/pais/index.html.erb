<p id="notice"><%= notice %></p>

<h1>Pais</h1>

<table>
  <thead>
    <tr>
      <th>Iso</th>
      <th>Name</th>
      <th>Nicename</th>
      <th>Iso3</th>
      <th>Numcode</th>
      <th>Phonecode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pais.each do |pai| %>
      <tr>
        <td><%= pai.iso %></td>
        <td><%= pai.name %></td>
        <td><%= pai.nicename %></td>
        <td><%= pai.iso3 %></td>
        <td><%= pai.numcode %></td>
        <td><%= pai.phonecode %></td>
        <td><%= link_to 'Show', pai %></td>
        <td><%= link_to 'Edit', edit_pai_path(pai) %></td>
        <td><%= link_to 'Destroy', pai, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pai', new_pai_path %>
